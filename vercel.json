{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      }
    ]
  }
  